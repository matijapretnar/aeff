operation task :   int * [|int -> int|]
operation result : int * int

let boxedFunc = [|fun a -> a+a|]

let parallelMap f seznam = 
    map awaitValue (
        map (fun e -> 
                let result = promise (result (x,r) when x = e ->
                    <<r>>
                ) in
                spawn(
                    unbox f as [|f'|] in
                    send result (e, (f' e))
                );
                result
            )
        seznam
    )


run parallelMap boxedFunc [1;2;3;4;5]